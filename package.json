{
  "name": "react-server-side-rendering-example",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/RowanCarmichael/react-server-side-rendering-example",
  "author": "Rowan Carmichael <carmichaelr@hotmail.co.uk>",
  "license": "MIT",
  "scripts": {
    "dev": "next"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "next": "^6.1.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.4.5"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
